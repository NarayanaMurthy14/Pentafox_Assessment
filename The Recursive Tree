function buildTree(comments) {
  const map = {};
  const result = [];
  comments.forEach(c => {
    map[c.id] = { ...c, children: [] };
  });
  comments.forEach(c => {
    if (c.parentId === null) {
      result.push(map[c.id]);
    } else {  map[c.parentId].children.push(map[c.id];
    }
    });
  return result;
}
